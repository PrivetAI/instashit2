# Пример файла конфигурации для Instagram Scraper

# OpenAI API ключ (обязательно)
OPENAI_API_KEY=your_openai_api_key_here

# Настройки базы данных
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/instagram_scraper

# Настройки Android эмулятора для скрапинга
ANDROID_HOST=localhost
ANDROID_PORT=4723

# Настройки приложения
NODE_ENV=development
PORT=5000

# Настройки для Docker (используются в docker-compose.yml)
# POSTGRES_DB=instagram_scraper
# POSTGRES_USER=postgres  
# POSTGRES_PASSWORD=postgres